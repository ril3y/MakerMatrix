{
  "name": "discogs-mcp-server",
  "description": "A MCP server for Discogs API integration",
  "vendor": "Christopher Kim (https://github.com/cswkim)",
  "sourceUrl": "https://github.com/cswkim/discogs-mcp-server",
  "homepage": "https://github.com/cswkim/discogs-mcp-server",
  "license": "MIT",
  "runtime": "node",
  "environmentVariables": {
    "DISCOGS_PERSONAL_ACCESS_TOKEN": {
      "description": "Personal access token for Discogs API",
      "required": true
    },
    "DISCOGS_API_URL": {
      "description": "API URL for Discogs API",
      "required": false
    },
    "DISCOGS_MEDIA_TYPE": {
      "description": "Accept header for Discogs API",
      "required": false
    },
    "DISCOGS_USER_AGENT": {
      "description": "User agent header for Discogs API",
      "required": false
    },
    "PORT": {
      "description": "Port for the MCP server SSE endpoint",
      "required": false
    },
    "SERVER_NAME": {
      "description": "Name of the MCP server",
      "required": false
    }
  }
}